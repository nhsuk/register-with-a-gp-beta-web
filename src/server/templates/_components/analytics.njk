<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

  ga('create', '{{ GA_ID() }}');
  ga('send', 'pageview');
  function ga_event(Category, Text, Label=''){
    ga('send', 'event', Category, Text, Label);
  }
  const eventsJson = '{{ events|safe }}';
  if(eventsJson !== ''){
    const events = JSON.parse(eventsJson);
    {% if events %}
        if(Array.isArray(events)) {
          for(i=0; i<events.length; i++){
            ga_event(events[i].ec, events[i].ea, events[i].el);
          }
        } else if(typeof events === 'object') {
            ga_event(events.ec, events.ea, events.el);
        }
    {% endif %}
  }
</script>
<script type="text/javascript" src="/static/webtrends.load.js"></script>